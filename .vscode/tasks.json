{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"env": {"RUST_BACKTRACE": "1"},
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo run"
		},
		{
			"type": "cargo",
			"command": "check",
			"env": {"RUST_BACKTRACE": "1"},
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo check"
		}
	]
}